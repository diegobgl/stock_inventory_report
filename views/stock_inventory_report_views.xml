<odoo>

    <!-- Añadir el componente OWL a la vista -->
    
    <record id="view_stock_inventory_report_tree" model="ir.ui.view">
        <field name="name">stock.inventory.report.tree</field>
        <field name="model">stock.inventory.report</field>
        <field name="arch" type="xml">
            <div>
                <div class="o_dashboard_wrapper">
                    <t t-component="inventory.InventoryDashboard"/>
                </div>
                <!-- Aquí va tu vista de lista -->
                <tree string="Reporte de Inventario a Fecha">
                    <field name="product_id"/>
                    <field name="location_id"/>
                    <field name="quantity"/>
                    <field name="date"/>
                </tree>
            </div>
        </field>
    </record>
</odoo>